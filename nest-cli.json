{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fisher-man-app/src",
  "monorepo": true,
  "root": "apps/fisher-man-app",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/fisher-man-app/tsconfig.app.json"
  },
  "projects": {
    "fisher-man-app": {
      "type": "application",
      "root": "apps/fisher-man-app",
      "entryFile": "main",
      "sourceRoot": "apps/fisher-man-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fisher-man-app/tsconfig.app.json"
      }
    },
    "fisher-man-service": {
      "type": "application",
      "root": "apps/fisher-man-service",
      "entryFile": "main",
      "sourceRoot": "apps/fisher-man-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fisher-man-service/tsconfig.app.json"
      }
    },
    "user-center-service": {
      "type": "application",
      "root": "apps/user-center-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-center-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-center-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "class": {
      "type": "library",
      "root": "libs/class",
      "entryFile": "index",
      "sourceRoot": "libs/class/src",
      "compilerOptions": {
        "tsConfigPath": "libs/class/tsconfig.lib.json"
      }
    },
    "tool": {
      "type": "library",
      "root": "libs/tool",
      "entryFile": "index",
      "sourceRoot": "libs/tool/src",
      "compilerOptions": {
        "tsConfigPath": "libs/tool/tsconfig.lib.json"
      }
    },
    "decorator": {
      "type": "library",
      "root": "libs/decorator",
      "entryFile": "index",
      "sourceRoot": "libs/decorator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decorator/tsconfig.lib.json"
      }
    }
  }
}